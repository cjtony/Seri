 <!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="pragma" content="no-cache" />
    <meta http-equiv="expires" content="-1" />
    <title>@ViewBag.Title</title>
    <link rel="icon" type="image/png" href="~/Content/img/icon-payroll.png" />
    <link rel="stylesheet" href="~/Content/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/styles/login.css" />
    <link rel="stylesheet" href="~/Content/animate/animate.css" />
    <link rel="stylesheet" href="~/Content/datatables-new/sb-admin-2.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</head>
<body>
    <div class="body-content">

        @RenderBody()

        <hr />

        <div class="container">
            <footer class="text-center p-4">
                <p class="title-font font-size-footer">
                    <i class="fas fa-copyright mr-05-ico ml-05-ico"></i> @DateTime.Now.Year - Raci TI Todos los derechos reservados.
                    <i class="fas fa-flag mr-05-ico ml-05-ico"></i> Icons by <a href="https://fontawesome.com/"> FontAwesome</a>.
                </p>
            </footer>
        </div>

    </div>

    <script type="text/javascript" src="~/Content/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/fontawesome/fontawesome.js"></script>
    <script type="text/javascript" src="~/Scripts/sweetalert/sweetalert.min.js"></script>
</body>
</html>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>